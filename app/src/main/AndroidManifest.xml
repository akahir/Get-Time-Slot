<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gettimeslot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DepartmentActivity"></activity>
        <activity android:name=".StudentSlotListActivity" />
        <activity android:name=".FacultySlotListActivity" />
        <activity android:name=".AddSlotActivity" />
        <activity android:name=".CreateAccountFacultyActivity" />
        <activity android:name=".CreateAccountStudentActivity" />
        <activity android:name=".LoginFacultyActivity" />
        <activity android:name=".LoginStudentActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>